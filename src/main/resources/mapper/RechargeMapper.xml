<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.newcompare.mapper.RechargeMapper">


    <select id="queryRecharge" resultType="com.example.newcompare.entity.Recharge">
        select * from recharge where out_trade_no=#{oid}
    </select>

    <select id="getById" resultType="com.example.newcompare.entity.Recharge">
        select * from recharge where id=#{id}
    </select>


</mapper>
