<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.compare.mapper.OrderLogMapper">

    <select id="getCompareIdByOrderId" resultType="java.lang.String">
        select c.id from order_log o
        left join compare c
        on c.order_id = o.id
        where o.out_trade_id = #{id}
    </select>
</mapper>
